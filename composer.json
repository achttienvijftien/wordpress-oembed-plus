{
    "name": "achttienvijftien/wordpress-oembed-plus",
    "description": "WordPress oEmbed Plus",
    "keywords" : ["wordpress", "plugin"],
    "homepage" : "https://www.1815.nl",
    "type": "wordpress-plugin",
    "license" : "GPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "AchttienVijftien\\Plugin\\OembedPlus\\": "src/"
        }
    },
    "authors": [
        {
            "name": "1815",
            "email": "it@1815.nl",
            "homepage" : "https://www.1815.nl"
        }
    ],
    "require": {
        "php": ">=8.3"
    },
    "require-dev": {
        "achttienvijftien/building-techniques": "^1.1",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.1",
        "phpcompatibility/phpcompatibility-wp": "^2.1",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "scripts": {
        "format": "phpcbf --report-summary --report-source",
        "lint": "phpcs"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
